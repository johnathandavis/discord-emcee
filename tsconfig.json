  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["ES2016", "ES2021.String"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist/",
    "strict": true,
